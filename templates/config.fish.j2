{% if 'config' in fish %}
{{ fish['config'] }}
{% endif %}

# load additional configuration files
for file in (find "$HOME/.config/fish/fish.d" -iname "*.fish" -type f) (find "$HOME/.config/fish/fish.d" -iname "*.fish" -type l)
  source $file
end
